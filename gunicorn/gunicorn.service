[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/bidstake-test
ExecStart=/home/ubuntu/.local/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/bidstake-test/auctionprop/auctionprop.sock auctionprop.wsgi:application

[Install]
WantedBy=multi-user.target
