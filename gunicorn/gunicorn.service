[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/bidstake-test
ExecStart=/usr/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/bidstake-test/auctionprop/auctionprop.sock bidstake.wsgi:application
       
[Install]
WantedBy=multi-user.target
